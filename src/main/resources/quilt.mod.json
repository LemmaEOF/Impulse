{
  "schema_version": 1,
  "quilt_loader": {
    "group": "gay.lemmaeof",
    "id": "impulse",
    "version": "${version}",
    "metadata": {
      "name": "Impulse",
      "icon": "assets/impulse/icon.png",
      "description": "Assorted little utilities for Minecraft mods to work with.",
      "license": "MIT",
      "contact": {
        "sources": "https://github.com/LemmaEOF/Impulse"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "gay.lemmaeof.impulse.Impulse"
      ],
      "client_init": [
        "gay.lemmaeof.impulse.ImpulseClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      }
    ]
  },
  "mixin": "mixins.impulse.json"
}
